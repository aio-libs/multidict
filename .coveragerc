[html]
show_contexts = true
skip_covered = false

[paths]
source =
  multidict
  */lib/pypy*/site-packages/multidict
  */lib/python*/site-packages/multidict
  */Lib/site-packages/multidict

[report]
exclude_also =
  pass
  ^\s*@pytest\.mark\.xfail
# fail_under = 98.95
skip_covered = true
skip_empty = true
show_missing = true

[run]
branch = true
cover_pylib = false
# https://coverage.rtfd.io/en/latest/contexts.html#dynamic-contexts
# dynamic_context = test_function  # conflicts with `pytest-cov` if set here
parallel = true
# plugins =
#   covdefaults
relative_files = true
source =
  .
source_pkgs =
  multidict
